{
  "name": "light-bootstrap-dashboard-react",
  "version": "2.0.1",
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^35.1.0",
    "@ckeditor/ckeditor5-react": "^5.0.2",
    "@fortawesome/fontawesome-free": "6.2.0",
    "axios": "^0.27.2",
    "bootstrap": "5.2.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9",
    "react": ">=18.2.0",
    "react-bootstrap": "2.5.0",
    "react-confirm-alert": "^3.0.6",
    "react-dom": ">=18.2.0",
    "react-dotenv": "^0.1.3",
    "react-router": "6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "reactstrap": "9.1.4",
    "sass": "1.54.8"
  },
  "optionalDependencies": {
    "jquery": "3.6.1",
    "typescript": "4.8.2"
  },
  "scripts": {
    "start": "react-dotenv && react-scripts start",
    "build": "react-dotenv && react-scripts build",
    "test": "react-dotenv && react-scripts test",
    "eject": "react-dotenv && react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "sass src/assets/scss/light-bootstrap-dashboard-pro-react.scss src/assets/css/light-bootstrap-dashboard-pro-react.css",
    "minify:scss": "sass src/assets/scss/light-bootstrap-dashboard-pro-react.scss src/assets/css/light-bootstrap-dashboard-pro-react.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss"
  },
  "proxy": "http://localhost:5000",
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
